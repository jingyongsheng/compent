  </parent>
  <modelVersion>4.0.0</modelVersion>
  <artifactId>compent</artifactId>
  <name>${project.artifactId}</name>
  <build>
    <plugins>
      <plugin>
        <artifactId>maven-source-plugin</artifactId>
        <executions>
          <execution>
            <id>attach-sources</id>
            <phase>none</phase>
          </execution>
        </executions>
      </plugin>
      <plugin>
        <artifactId>maven-javadoc-plugin</artifactId>
        <executions>
          <execution>
            <id>attach-javadoc</id>
            <phase>deploy</phase>
            <goals>
              <goal>jar</goal>
            </goals>
          </execution>
        </executions>
      </plugin>
      <plugin>
        <artifactId>maven-shade-plugin</artifactId>
        <version>1.4</version>
        <executions>
          <execution>
            <phase>package</phase>
            <goals>
              <goal>shade</goal>
            </goals>
            <configuration>
              <createSourcesJar>false</createSourcesJar>
              <promoteTransitiveDependencies>true</promoteTransitiveDependencies>
              <artifactSet>
                <includes>
                  <include>com.zcbl:compent-api</include>
                  <include>com.zcbl:compent-client-api</include>
                  <include>com.zcbl:compent-client-core</include>
                  <include>com.zcbl:compent-client-impl</include>
                  <include>com.zcbl:compent-function-impl</include>
                  <include>com.zcbl:compent-language-repertories</include>
                  <include>com.zcbl:compent-client-quartz</include>
                  <include>com.zcbl:compent-client-restful</include>
                  <include>com.zcbl:compent-data-center-api</include>
                  <include>com.zcbl:compent-data-center-client</include>
                  <include>com.zcbl:compent-data-center-impl</include>
                  <include>com.zcbl:compent-data-center-log</include>
                  <include>com.zcbl:compent-db-client</include>
                  <include>com.zcbl:compent-db-pool</include>
                  <include>com.zcbl:compent-config</include>
                  <include>com.zcbl:compent-client-restful</include>
                </includes>
              </artifactSet>
              <transformers />
            </configuration>
          </execution>
        </executions>
      </plugin>
    </plugins>
  </build>
